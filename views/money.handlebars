<html lang="en" class="theme-light">
<head>
  <meta charset="utf-8"/>
  <title>MemoryBoard</title>
  {{!-- <link rel="shortcut icon" href="/assets/img/favicon.png"/> --}}
  <link rel="stylesheet" type="text/css" href="/assets/style.css"/>
  <link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/bulma@1.0.0/css/bulma.min.css"
/>

</head>
<body> 
  
  <section class="columns has-background-dark">
    <div class="column is-3 section is-sticky has-background-white">
      <a class="has-text-centered" href="/">
        <figure class="image container is-96x96">
          <img id="logo" src="/assets/img/polaroid2.png" alt="camera logo"/>
        </figure>
      </a>
      <p class="title has-text-centered pt-4">MemoryBoard</p>
      <aside class="menu">
        <p class="menu-label is-hidden-touch"></p>
        <ul class="menu-list">
          <li>
            <a href="/createPost" class="has-text-left p-4">
              <span class="icon-text">
                  <span class="icon">
                    <span class="ph--house-fill"></span>
                  </span>
                  <span>Home</span>
              </span>
            </a>
          </li>
          <li>
            <a href="/premium" class="has-text-left p-4">
              <span class="icon-text">
                  <span class="icon">
                    <span class="mdi--sparkles"></span>
                  </span>
                  <span>Premium</span>
              </span>
            </a>
          </li>
          <li>
            <a href="/logout" class="has-text-left p-4">
              <span class="icon-text">
                  <span class="icon">
                    <span class="solar--logout-3-bold"></span>
                  </span>
                  <span>Log out</span>
              </span>
            </a>
          </li>
        </ul>
      </aside>
      
    </div>

    <div class="column is-6">
      <section id="content3"></section>
    </div>
